<view style="min-height: 100%; background-color: #F3F3F3">
    <ui-tabs wx:if='{{itemClassList.length}}' index="{{ current }}" bindchange="handleChange" class="bgfff" ink-bar="true" ink-bar-style="{{inkBarStyle}}" _pid="{{_pid}}">
        <ui-tab wx:for='{{itemClassList}}'>{{item.itemClassName}}</ui-tab>
    </ui-tabs>
    <view class="clearfix">
        <view wx:for='{{list}}' data-index="{{index}}" data-id='{{item.id}}' catchtap="itemClick" style="border-color:{{item.checked?'#459E8C':'#fff'}}" class="pad10 tjxmWap bgfff clearfix btm5">
            <view url="projectDetail?itemId={{item.id}}" class=" right10 left" style="width: 183rpx">
                <image mode="aspectFill" src="{{item.imgs}}" class="img " />
            </view>
            <view style="width: 482rpx" class="left">
                <view class=" bor_btm_so" style="padding-bottom: 10rpx">
                    <text class="col000 ">{{item.itemName}}</text>
                    <text class="left10  col000">默认：{{item.defaultDuration}}分钟</text>
                    <text class="left15 colPink font3">¥{{item.defaultPrice/1000 * item.defaultDuration}}</text>
                </view>
                <view class="top5 line24">{{item.conditioningMethod}}</view>
                <view class="top5">
                    <view class="tag1">好评9999+</view>
                </view>
                <view class="align-right">
                    <image src="../../static/images/30.png" style="width: 32rpx; height: 37rpx" class="icoAll" />
                    <image src="../../static/images/31.png" style="width: 34rpx; height: 33rpx" class="icoAll left10" />
                    <navigator wx:if="{{pageFrom=='group'}}" url='../wode/doGroup' class="btns btns-small btns-green left10" >发起拼图</navigator>
                </view>
            </view>
        </view>
    </view>
    <view bindtap="confirmChoosen" class="btns btns-biggest btns-green " style="position: fixed; left: 0; bottom: 0;">确认选择</view>
</view>