<view style="min-height:100%; background-color: rgba(243,243,243,1) ">
    <image src="../../static/images/20.png" style="width: 100%; height:386rpx; " />
    <view class=" pad20" style="padding-bottom: 100rpx;">
        <view>
            <text class="col333">服务门店：</text>中央商场旗舰店
        </view>
        <view class="top10">
            <text class="col333">营业时间：</text>10:00-10:00
        </view>
        <view class="top10">
            <text class="col333">预约日期：</text>
                <view class="checkBtn {{cIndex==0?'active':''}}" data-index="0" bindtap="chose">今天</view>
                <view class="checkBtn left10 {{cIndex==1?'active':''}}" data-index="1" bindtap="chose">明天</view>
            


        </view>
        <view class="top10">
            <picker class="inline" mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
                <text class="col333">预约时间：</text>{{time?time:'请选择时间'}}
            </picker>
        </view>
    </view>
    <navigator open-type="redirect" url="projectsChoose" class="btns btns-biggest btns-green " style="position: fixed; left: 0; bottom: 0;">立即预约</navigator>
</view>