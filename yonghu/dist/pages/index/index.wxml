<view style="min-height: 100%; padding-bottom: 54px; box-sizing: border-box; background-color:#F3F3F3;">
    <swiper class="swiper" indicator-dots="true"
    autoplay="true">
        <block wx:for="{{D.banners}}">
            <swiper-item>
                <image src="{{item.path}}" mode="aspectFit" />
            </swiper-item>
        </block>
    </swiper>
    <view class="box1 bgfff claerfix">
        <image src="../../static/images/15.png" class="right ico15 top18 right20" />
        <image src="../../static/images/14.png" class="ico14 left top12 left20" />
        <text class="left20">今日有更多套餐可享受优惠活动</text>
    </view>
    <view class="padSp1">
        <view class="title font2 col000">服务分类</view>
        <view class="top15 clearfix">
            <view wx:for='{{itemClassList}}' class="left right20" style="">
                <navigator url="projectsList?id={{item.id}}&pageFrom=index">
                    <image src="{{item.icon}}" class="ico1" mode='aspectFit' />
                    <view class="top5 col999">{{item.itemClassName}}</view>
                </navigator>
            </view>
        </view>
        <view class="title top10 font2 col000">推荐项目</view>
    </view>
    <navigator url="projectDetail?itemId={{item.id}}" wx:for='{{D.itemRecommendList}}' class="pad10 tjxmWap bgfff top5 clearfix" data-index="{{index}}" data-id="{{item.id}}" catchtap="chooseProject">
        <view class=" right10 left" style="width: 183rpx">
            <image src="{{item.imgs}}" class="img " mode='aspectFill' />
        </view>
        <view style="width: 482rpx" class="left">
            <view class=" bor_btm_so" style="padding-bottom: 10rpx">
                <text class="col000 ">{{item.itemName}}</text>
                <text class="left10  col000">默认：{{item.defaultDuration}}分钟</text>
                <text class="left15 colPink font3">¥{{item.defaultPrice/1000 * item.defaultDuration}}</text>
            </view>
            <view class="top5 line24">{{item.conditioningMethod}}</view>
        </view>
    </navigator>
    <view class="padSp1 clearfix">
        <navigator url="shopList?pageFrom=index" class="right top2">更多 ></navigator>
        <text class="title font2 col000 ">附近门店</text>
    </view>
    <view class="bgfff" style="padding: 0 40rpx">
        <!--选择门店预约-->
        <view wx:for='{{D.nearbyStore}}' class="fjmdWap bgfff bor_btm_so" data-index='{{index}}' data-id="{{item.id}}" catchtap='chooseStore'>
            <view>
                <image src="{{item.imgs}}" class="img left right10" mode='aspectFill' />
            </view>
            <view class="">
                <text class="col000 font2">{{item.name}}</text>
                <view class="right ">
                    <text class="col999 verMid font0">距您{{item.distance}}m | </text>
                    <image src="../../static/images/16.png" class="icoAll ico16 left5" />
                    <text class="colGreen left5 verMid font0">咨询</text>
                </view>
            </view>
            <view class="">
                <image src="../../static/images/12.png" class="ico12 icoAll" />
                <text class="col999 left5 font0 verMid">{{item.openStartTime || '10:00'}}-{{item.openEndTime || '23:00'}}</text>
            </view>
            <view class="line0 ">
                <image src="../../static/images/11.png" class="ico11 icoAll" />
                <text class="col999 left5 font0 verMid">{{item.detailAddress}}</text>
            </view>
        </view>

    </view>
    <button open-type="contact" class='ico4 icoAll'><image src="../../static/images/4.png" style='width: 110rpx; height: 110rpx;' /></button>
    
    <ui-popup show="{{jxzShow}}" background="transparent" _pid="{{_pid}}">
        <view class="padSp1 clearfix" style="background-color:#fff; border-radius: 10rpx 10rpx 0 0; overflow: hidden;">
            <view class="bor_btm_so" style="padding-bottom: 20rpx;">
                <text class="font2 bold col333">进行中</text>
                <navigator class="top5 right" url="pingjia"> <image src="../../static/images/17.png" class="ico17 " /></navigator>
            </view>
            <view class="col333 font2 top15 bold">编号：Z100201916049325</view>
            <view class="clearfix top15">
                <image src="../../static/images/13.png" class="tx1 left right15" />
                <view class="top10">
                    <text class="col333 font2">【颈椎放松】</text>
                    <image src="../../static/images/12.png" class="ico12 icoAll left10" />
                    <text class="left5 verMid">推荐：</text>
                    <text class="colRed">60分钟</text>
                </view>
                <view class="top5 font0">这是一段介绍，足疗30分钟，颈部按摩20分钟</view>
            </view>
            <view class="top15 col333 font2 bold">
                中央商场 旗舰店
            </view>
            <ui-timeline class="top10" style="width:100%" _pid="{{_pid}}">
                <ui-timeline-item wx:for="{{lcList}}" wx:key="index" point-color="#459E8C" line-color="#BFBFBF" show-line="{{index === (lcList.length - 1) ? false : true}}" shadow="{{false}}" _pid="{{_pid}}">
                    <view class="left40 relative" style="padding: 30rpx 0">
                        <view class="absolute" style="top: -8rpx">{{item.content}}</view>
                    </view>
                </ui-timeline-item>
            </ui-timeline>
            <view class=" col333 font2 bold" style="margin-top: -10rpx">
                已购商品：
            </view>
            <view class="top5 ">矿泉水*2</view>
            <view class="">矿泉水*2</view>
            <view class="top10 align-right">
                <view class="btns btns-small btns-green">续时</view>
                <navigator url="goods" class="btns btns-small btns-green left20">商品</navigator>
            </view>
        </view>


    </ui-popup>
</view>