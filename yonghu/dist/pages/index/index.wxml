<view style="height: 100%; overflow:{{overflow}}; padding-bottom: 54px; box-sizing: border-box; background-color:#F3F3F3;">
    <swiper class="swiper" indicator-dots="true"
    autoplay="true">
        <block wx:for="{{D.banners}}">
        <!-- 类别（1 门店,2项目,3技师,4链接5.无链接 -->
            <swiper-item>
                <image bindtap="bannerGo" data-index="{{index}}" src="{{item.path}}" mode="aspectFit" />
            </swiper-item>
        </block>
    </swiper>
    <view class="box1 bgfff claerfix" style='overflow: hidden'>
        <image src="../../static/images/14.png" class="ico14 left top12 left20" />
        <view style='width: 480rpx; overflow: hidden' class='inline'>
            <view class="marquee_box">
                 <view class="marquee_text" style="{{orientation}}:{{marqueeDistance}}px; font-size: {{size}}px;">
                  {{D.notic}}
                 </view>
             </view>
        </view>
        <!-- <ui-roller _pid="{{_pid}}" style='width: 480rpx;'  class='left10' autoplay='3000'>
            <ui-roller-item _pid="{{_pid}}">
                <view>{{D.notic}}</view>
            </ui-roller-item>
            <ui-roller-item _pid="{{_pid}}">
                <view>今日有更多套餐可享受优惠活动</view>
            </ui-roller-item>
        </ui-roller> -->
    </view>
    <view class="padSp1">
        <view class="title font2 col000">服务分类</view>
        <view class="top15 clearfix">
            <view wx:for='{{itemClassList}}' class="left right20" style="">
                <view bindtap="goProjectsList" data-id="{{item.id}}" url="projectsList?id={{item.id}}&pageFrom=index">
                    <image src="{{item.icon}}" class="ico1" mode='aspectFit' />
                    <view class="top5 col999">{{item.itemClassName}}</view>
                </view>
            </view>
        </view>
        <view class="title top10 font2 col000">推荐项目</view>
    </view>
    <view url="projectDetail?itemId={{item.id}}" wx:for='{{D.itemRecommendList}}' class="pad10 tjxmWap bgfff top5 clearfix" data-index="{{index}}" data-id="{{item.id}}" catchtap="chooseProject">
        <view class=" right10 left" style="width: 160rpx">
            <image src="{{item.imgs}}" class="img " mode='aspectFill' />
        </view>
        <view style="width: 520rpx" class="left">
            <view class=" bor_btm_so" style="padding-bottom: 10rpx">
                <text class="col000 inline" style='width: 180rpx'>{{item.itemName}}</text>
                <text class="left10  col000">默认：{{item.defaultDuration}}分钟</text>
                <text class="left15 colPink font3">¥{{item.defaultPrice/100}}</text>
            </view>
            <view class="top5 line24">{{item.conditioningMethod}}</view>
        </view>
    </view>
    <view class="padSp1 clearfix">
        <view bindtap="moreStore" url="shopList?pageFrom=index" class="right top2">更多 ></view>
        <text class="title font2 col000 ">附近门店</text>
    </view>
    <view class="bgfff" style="padding: 0 40rpx">
        <!--选择门店预约-->
        <view wx:for='{{D.nearbyStore}}' class="fjmdWap bgfff bor_btm_so" data-index='{{index}}' data-id="{{item.id}}" catchtap='chooseStore'>
            <view>
                <image src="{{item.imgs}}" class="img left right10" mode='aspectFill' />
            </view>
            <view class="">
                <text class="col000 font2">{{item.name}}</text>
                <view class="right ">
                    <text class="col999 verMid font0">距您{{item.distance}}km | </text>
                    <image src="../../static/images/16.png" class="icoAll ico16 left5" />
                    <text catchtap="makePhoneCall" data-phonenum="{{item.phoneNum}}" class="colGreen left5 verMid font0">咨询</text>
                </view>
            </view>
            <view class="">
                <image src="../../static/images/12.png" class="ico12 icoAll" />
                <text class="col999 left5 font0 verMid">{{item.openStartTime || '10:00'}}-{{item.openEndTime || '23:00'}}</text>
            </view>
            <view class="line0 ">
                <image src="../../static/images/11.png" class="ico11 icoAll" />
                <text class="col999 left5 font0 verMid">{{item.detailAddress}}</text>
            </view>
        </view>

    </view>
    <button open-type="contact" class='ico4 icoAll' style='left:{{left}}px; top:{{top}}px'  bindtouchmove="bindtouchmove" bindtouchstart="bindtouchstart" bindtouchend="bindtouchend"><image src="../../static/images/4.png" style='width: 55px; height: 55px; '     /></button>
    
    <ui-popup show="{{jxzShow}}" wx:if='{{O}}' background="transparent" _pid="{{_pid}}">
        <view class="padSp1 clearfix" style="background-color:#fff; border-radius: 10rpx 10rpx 0 0; overflow: hidden; padding-bottom: 100rpx;">
            <view class="bor_btm_so" style="padding-bottom: 20rpx;">
                <text class="col333 bold">{{statusName[O.status]}}</text>
                <!-- <navigator class="top5 right" url="pingjia"> <image src="../../static/images/17.png" class="ico17 " /></navigator> -->
            </view>
            <view class='pad10TB bor_btm_so' z>
                <view class=""><text class='bold'>编号：</text>{{O.outTradeNo}}</view>
                <view class="top5 "><text class='bold'>预约门店：</text>{{O.storeName}}</view>
            </view>
            <view class='pad10TB bor_btm_so' wx:for='{{O.orderItems}}'>
                <view class=" "><text class='bold'>预约项目：</text>{{item.itemName}}</view>
                <view class=" top5 "><text class='bold'>技师：</text><text wx:for='{{item.orderTechnicians}}' wx:for-item="tecItem">{{tecItem.employeeName}}</text></view>
                <view class="top5  "><text class='bold'>预约开始时间：</text>{{O.orderStartTime}}</view>
                <view class="top5  "><text class='bold'>预计结束时间：</text>{{O.orderEndTime}}</view>
            </view>

