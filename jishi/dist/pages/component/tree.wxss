/* pages/component/tree.wxss */
.stBox {
  position: relative;
  width: 100px;
  padding: 10px 0;
}
.stBox .a {
  width: 32px;
  float: left;
  text-indent: 4px;
  font-weight: bold;
  line-height: 28px;
  height: 32px;
  border: 3px solid #6d55f6;
  border-radius: 5px;
}
.stBox .b {
  width: 60px;
  margin: 5px 0 0 -15px;
  float: left;
  height: 25px;
  line-height: 25px;
  text-align: center;
  color: #fff;
  background: #6d55f6;
  border-radius: 5px;
  font-size: 12px;
}
.inline{ display: inline-block}